        {
          "_name": "share2",
          "_sourceTheme": "goalm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"social2\" group=\"Social\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" plugins=\"SocialLikes,Socicon\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n\n        <header>Icons</header>\n\n        <input type=\"color\" title=\"Background\" name=\"iconsColor\" value=\"#4f4c4d\">\n\n        <input type=\"range\" inline title=\"Quantity\" name=\"quantity\" min=\"1\" max=\"6\" step=\"1\" value=\"4\">\n\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/goalm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"col-12\">\n                <h3 class=\"\n                        mbr-section-title\n                        align-center\n                        mb-5\n                        mbr-fonts-style\n                    \" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .mbr-social-likes\">\n                    Share this Page!\n                </h3>\n                <div>\n                    <div class=\"mbr-social-likes\" data-counters=\"false\">\n                     \n                        <a class=\"\n                                iconfont-wrapper\n                                bg-twitter\n                                m-2\n                                btn-social\n                            \" target=\"_blank\" data-app-placeholder=\"Type Text\" href=\"https://twitter.com/lawy_AlEnezi\" mbr-if=\"quantity>1\">\n                            <span mbr-icon class=\"socicon-twitter socicon\"></span>\n                        </a>\n\n                        <a class=\"\n                                iconfont-wrapper\n                                bg-vkontakte\n                                m-2\n                                btn-social\n                            \" target=\"_blank\" data-app-placeholder=\"Type Text\" href=\"https://wa.link/vrn77r\" mbr-if=\"quantity>2\">\n                            <span mbr-icon class=\"socicon-whatsapp socicon\"></span>\n                        </a>\n\n                     \n\n                    \n\n                       \n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
